    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Inference",
            "type": "debugpy",
            "request": "launch",
            "program": "inference.py",
            "args": [
                "--config_file",
                "mm_interleaved/configs/release/mm_inference.yaml"
            ],
            "env": {
                "RANK": "0",
                "LOCAL_RANK": "0",
                "WORLD_SIZE": "1",
                "LOCAL_WORLD_SIZE": "1",
                "MASTER_ADDR": "localhost",
                "MASTER_PORT": "20143"
            },
            "console": "integratedTerminal",
        }
    ]
}